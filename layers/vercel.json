{
  "version": 2,
  "builds": [
    {
      "src": "bkndarchitechure/**",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/index",
      "dest": "/bkndarchitechure/app_apilayer.js"
    },
    {
      "src": "/(.*)",
      "dest": "/bkndarchitechure/app_apilayer.js"
    }
  ]
}
